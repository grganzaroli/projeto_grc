    <name>Rate</name>
    <key>rate</key>
    <type>int</type>
  </param>
  <param>
    <name>Mod_size</name>
    <key>mod_size</key>
    <type>int</type>
  </param>
  <param>
    <name>Var</name>
    <key>var</key>
    <type>float</type>
  </param>
  <sink>
    <name>in</name>
    <type>complex</type>
    <vlen>$size_in</vlen>
  </sink>
  <source>
    <name>out</name>
    <type>float</type>
    <vlen>$size_out</vlen>
  </source>
</block>
